{
    "name": "travismillerdesign-v5",
    "version": "1.0.0",
    "description": "v5 portfolio website",
    "main": "index.js",
    "scripts": {
        "compile:scss": "sass --load-path=node_modules --load-path=src/styles src/styles/project.scss src/styles/project.css --style=compressed",
        "watch:scss": "sass --load-path=node_modules --load-path=src/styles src/styles/project.scss src/styles/project.css --watch",
        "watch:11ty": "npx @11ty/eleventy --serve --quiet",
        "optimize:images": "node optimize-images.js",
        "prebuild": "npm run compile:scss",
        "build": "npx @11ty/eleventy && npm run optimize:images",
        "serve": "npm-run-all compile:scss --parallel watch:*"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/travismillerdesign/travismillerdesign-v5.git"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/travismillerdesign/travismillerdesign-v5/issues"
    },
    "homepage": "https://github.com/travismillerdesign/travismillerdesign-v5#readme",
    "dependencies": {
        "@11ty/eleventy": "^3.1.2"
    },
    "devDependencies": {
        "@11ty/eleventy-img": "^6.0.4",
        "modern-normalize": "^3.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^3.6.2",
        "sass": "^1.93.2"
    }
}
